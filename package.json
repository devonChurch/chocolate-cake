    "awesome-typescript-loader": "^5.2.1",
    "jest": "^23.6.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2"
